    "version": "0.2.0",
    "configurations": [
        {
            "name": "My App",
            "request": "launch",
            "type": "dart",
            "args": ["--web-hostname", "any", "--web-port", "3000"]
        },
        {
            "name": "My App (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--web-hostname=0.0.0.0", "--web-port=3000"]
        },
        {
            "name": "My App (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--web-hostname=0.0.0.0", "--web-port=3000"]
        }
    ]
}